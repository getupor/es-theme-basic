			extra="true"
			visible="				!empty({game:developer})
						&amp;&amp;	!empty({game:genre})
						&amp;&amp;	!empty({game:releaseyear})
						&amp;&amp;	!empty({game:publisher})
						&amp;&amp;	{game:developer} != {game:publisher}
					"
			
