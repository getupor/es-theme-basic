<?xml version="1.0" encoding="UTF-8"?>

<theme>

	<formatVersion>7</formatVersion>
	
	<view name="grid">

		<text name="gameName"
		
			extra="true"
			zIndex="1001"
			forceUppercase="true"
			alignment="center"
			verticalAlignment="center"
			fontPath="./../fonts/${f.mbi}"
			origin="0.5 0.5"
            pos="0.416 0.555"
            size="0.5 0.017"
            fontSize="0.016"
			color="${c.yellow}"
            glowColor="${c.glowColor}"
			glowOffset="2"
			glowSize="2"
			opacity="0"
			text="{game:name} ({game:system:name})"

			>
			
			<storyboard event="open">
			
				<animation property="opacity" to="1" mode="linear" />
				<animation property="opacity" to="0" mode="linear" duration="50" begin="500"/>
				
			</storyboard>
			
			<storyboard event="activateNext">
			
				<animation property="opacity" to="1" mode="linear" />
				<animation property="opacity" to="0" mode="linear" duration="50" begin="500"/>
				
			</storyboard>
			
			<storyboard event="activatePrev">
			
				<animation property="opacity" to="1" mode="linear" />
				<animation property="opacity" to="0" mode="linear" duration="50" begin="500"/>
				
			</storyboard>
			
			<storyboard event="deactivatePrev">
			
				<animation property="opacity" to="0" mode="linear" duration="10"/>
				
			</storyboard>
			
			<storyboard event="deactivateNext">
			
				<animation property="opacity" to="0" mode="linear" duration="10"/>
				
			</storyboard>

			<visible ifSubset="gridNames:no">false</visible>

			</text>
			
		<image name="frame"
		
			extra="true"
			zIndex="600"
			path="./../parts/white.png"
			origin="0.5 0.5"
			size="${v.frame.size}"
			pos="${v.frame.pos}"
			color="${c.frame}80"
			
		>
		
			<shader ifSubset="shaders:on">./../shaders/scanlines.glsl</shader>
		
		
		</image>

		<image name="frameBorders"
		
			extra="true"
			zIndex="599"
			path="./../parts/white.png"
			origin="0.5 0.5"
			size="${v.frameBorders.size}"
			pos="${v.frame.pos}"
			color="${c.main}33"
			
		/>
			
		<image name="frameShadow"
		
			extra="true"
			linearSmooth="true"
			zIndex="598"
			path="./../parts/frameShadow.png"
			origin="0.5 0.5"
			pos="0.5 0.5"
			size="1 1"
			color="${c.glowColor}"
			opacity="0.5"
			
		/>

		<image name="md_fanart"
		
			zIndex="601"
			ifSubset="fanarts:yes"
			linearSmooth="false"
			origin="0.5 0.5"
			minSize="${v.frame.size}"
			pos="${v.frame.pos}"
			saturation="0"
			color="ffffff0d"
			colorEnd="ffffff33"
			gradientType="horizontal"
			opacity="0.6"
			autoFade="true"
			
		>
		
			<shader ifSubset="shaders:on">./../shaders/scanlines.glsl</shader>
			<visible tinyScreen="true">false</visible>


		</image>

		<image name="logo"
		
			origin="0 0.5"
			x="0.025"
		
		/>

		<imagegrid name="gamegrid"
		
			zIndex="1001"
			pos="0.025 0.205"
			size="${v.frame.size}"
			margin="0.03 0.05"
			padding="0.025 0.04 0.205 0.03"
			autoLayout="4 2"
			animateSelection="false"
			centerSelection="false"
			scrollbarColor="${c.textHeader}"
			scrollbarCorner="0.9"
			scrollbarSize="${v.line.hc}"
			autoLayoutSelectedZoom="1.1"
			scrollbarAlignment="right"
			imageSource="image"
			scrollSound="./../parts/scroll.wav"

		>
		
			<autoLayout if="${s.notWide}">3 2</autoLayout>
			<autoLayout if="${s.ultraWide}">5 2</autoLayout>
			<imageSource tinyScreen="true">thumbnail</imageSource>
			<margin tinyScreen="true">0.02 0.03</margin>
			<padding tinyScreen="true">0.025 0.04 0.025 0.03</padding>
			<showVideoAtDelay ifSubset="gridVideos:yes">3</showVideoAtDelay>
		</imagegrid>

		<gridtile name="default"
		
			backgroundColor="00000000"
			backgroundEdgeColor="00000000"
			backgroundCenterColor="00000000"
			backgroundCornerSize="9 9"
			selectionMode="image"
			imageSizeMode="minSize"
			padding="6 6"
			imageColor="ffffff"
			reflexion="0 0"
			
		>
		
			<imageSizeMode tinyScreen="true">maxSize</imageSizeMode>
		
		</gridtile>

		<gridtile name="selected"
		
			backgroundCenterColor="${c.textHeader}"
			backgroundEdgeColor="${c.textHeader}"
			imageColor="ffffff"
			
		/>

		<text name="gridtile"
		
			fontPath="./../fonts/${f.mb}"
			fontSize="0.016"
			color="${c.text}"
			extraTextColor="${c.textHeader}"
			glowColor="${c.glowColor}"
			glowSize="1"
			glowOffset="2"
			verticalAlignment="center"
			alignment="left"
			forceUppercase="true"
			
		>
		
			<visible tinyScreen="true">false</visible>
		
		</text>

		<text name="gridtile:selected"
		
			color="${c.selected}"
			
	   >
	   
			<visible tinyScreen="true">false</visible>
	   
	   </text>

		<image name="gridtile.favorite"
		
			path="./../parts/favorite.png"
			pos="0 0"
			maxSize="0.13 0.13"
			origin="0.5 0.5"
			
		>
		
			<maxSize tinyScreen="true">0.2 0.2</maxSize>
		
		</image>

		<image name="gridtile.image"
		
			linearSmooth="true"
			roundCorners="0.02"
			
		/>  

		<ninepatch name="gridtile.background:selected"
		
			animateColor="${c.menuSelector}"
			animateColorTime="160"
		/>

		<image name="md_marquee"
			zIndex="1001"
			linearSmooth="true"
			saturation="1.1"
			maxSize="0.1546875 0.1083333333333333"
			origin="0.5 0.5"
			pos="0.8799479166666667 0.2837962962962963"

		>
		
			<shader ifSubset="shaders:on">./../shaders/shadowMarquee.glsl</shader>		
			<maxSize tinyScreen="true">0.28 0.1203703703703704</maxSize>
			<origin tinyScreen="true">1 0.5</origin>
			<x tinyScreen="true">0.975</x>
			<y tinyScreen="true">0.12</y>
		
		</image>

		<image name="md_flag"
		
			zIndex="1010"
			linearSmooth="true"
			origin="0.5 0.5"
			pos="0.8799479166666667 0.363"
			maxSize="0.0286458333333333 0.025"
			roundCorners="0.018"

		>
		
			<x tinyScreen="true">0.6</x>
			<y tinyScreen="true">0.1</y>
			<maxSize tinyScreen="true">0.044 0.04</maxSize>

		</image>
		
		<image name="md_thumbnail,md_image"
		
			zIndex="1001"
			linearSmooth="true"
			saturation="1.1"
			origin="0.5 0"
			pos="0.8799479166666667 0.6740740740740741"
			maxSize="0.1369791666666667 0.2259259259259259"
			roundCorners="0.018"
			opacity="0"
			scale="0"
			
		>

			<storyboard event="open">
				<animation property="scale" from="0" to="1" mode="linear" duration="1"/>
				<animation property="opacity" from="0" to="0" mode="linear" duration="10"/>
				<animation property="opacity" from="0" to="1" mode="linear" duration="50" begin="10"/>
				<animation property="opacity" to="0" mode="linear" duration="250" begin="1250"/>
			</storyboard>
			<storyboard event="activateNext">
				<animation property="scale" from="0" to="1" mode="linear" duration="1"/>
				<animation property="opacity" from="0" to="0" mode="linear" duration="10"/>
				<animation property="opacity" from="0" to="1" mode="linear" duration="50" begin="10"/>
				<animation property="opacity" to="0" mode="linear" duration="250" begin="1250"/>
			</storyboard>
			<storyboard event="activatePrev">
				<animation property="scale" from="0" to="1" mode="linear" duration="1"/>
				<animation property="opacity" from="0" to="0" mode="linear" duration="10"/>
				<animation property="opacity" from="0" to="1" mode="linear" duration="50" begin="10"/>
				<animation property="opacity" to="0" mode="linear" duration="250" begin="1250"/>
			</storyboard>
			<storyboard event="deactivatePrev">
				<animation property="opacity" to="0" mode="linear" duration="10"/>
			</storyboard>
			<storyboard event="deactivateNext">
				<animation property="opacity" to="0" mode="linear" duration="10"/>
			</storyboard>

			<visible tinyScreen="true">false</visible>
		
		</image>
		
		<image name="md_image"
		
			visible="false"
			
		>

			<shader ifSubset="shaders:on">./../shaders/scanlinesBright.glsl</shader>		

		</image>
		
		<text name="md_description"
		
			zIndex="1001"
			forceUppercase="false"
			alignment="center"
			verticalAlignment="top"
			fontPath="./../fonts/${f.mri}"
			origin="0.5 0"
            pos="0.8799479166666667 0.6648148148148148"
            size="0.1546875 0.2351851851851852"
            fontSize="0.024"
			color="${c.selected}"
            glowColor="${c.glowColor}"
			glowOffset="2"
			glowSize="1"
			autoScrollSpeed="50"
			lineSpacing="1.37"
			
			/>
						
		<text name="noDescription"
		
